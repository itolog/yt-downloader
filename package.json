{
  "name": "downloader-ytb",
  "description": "video downloader",
  "author": "gamonya jeden",
  "version": "1.0.0",
  "private": true,
  "main": "src/main/main.js",
  "build": {
    "productName": "ytb-downloader",
    "appId": "org.ytbitdown.id",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "src/assets"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.41",
    "fluent-ffmpeg": "^2.1.2",
    "moment": "^2.24.0",
    "move-file": "^1.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "sanitize-filename": "^1.6.3",
    "typeface-roboto": "0.0.75",
    "ytdl-core": "^1.0.7",
    "electron-is-dev": "^1.1.0",
    "electron-reload": "^1.5.0"
  },
  "scripts": {
    "gulp": "gulp",
    "start": "react-scripts start",
    "react-build": "react-scripts build",
    "el-build": "electron-builder",
    "electron-pack": "build --em.main=build/electron.js",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "build": "yarn react-build && yarn el-build build --win --x64",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "el-start": "concurrently \"tsc ./src/main/main.ts -w\" \"cross-env NODE_ENV=dev nodemon --exec \"\"wait-on http://localhost:3000 && electron src/main/main.js\"\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.26",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "concurrently": "^5.1.0",
    "cross-env": "^6.0.3",
    "electron": "^9.4.0",
    "electron-builder": "^22.3.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.5",
    "wait-on": "^3.3.0"
  }
}
